@model RentCar.Models.CR_Cas_Sup_Branch_Documentation

@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_CASLayout.cshtml";
}

<link href="~/BSys3Styles/CSS/style_form.css" rel="stylesheet" />
<div class="content">

    <!-- Start Content-->
    <div class="container-fluid">

        <div class="row">
            <div class="col-12">
                <div class="card-box table-responsive" style="height:79vh">
                    <div class="title_corp" ><h3> الشركة /العقود / تحديث</h3></div>
                    <div class="subtitle_corp">تمكنك هذه الصفحة من تحديث  بيانات العقود</div>
                    <div style="float:left; margin-left:50px;margin-top:-85px;" class="zoom">
                        @*@if (Model.CR_Mas_Sup_Category_Car_Picture != null)
                            {
                                <img src="@Url.Content(Model.CR_Mas_Sup_Category_Car_Picture)" id="output" width="110" height="80" />
                            }*@
                    </div>
                    <div class="hr_60"></div>
                    <div class="hr_80" style=""></div>
                    <div class="corp_page " style=" padding-top: 20px;">
                        <div class="page_content" >

                            @using (Html.BeginForm())
                            {
                                @Html.AntiForgeryToken()

                            <div class="form-horizontal" style="min-height:70%">

                                @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                                @Html.HiddenFor(model => model.CR_Cas_Sup_Branch_Documentation_Lessor_Code)

                                @Html.HiddenFor(model => model.CR_Cas_Sup_Branch_Documentation_Branch_Code)

                                @Html.HiddenFor(model => model.CR_Cas_Sup_Branch_Documentation_Code)


                                <div class="row">
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <div class="row">
                                                <label class="col-md-6 label6"> نوع العقد </label>
                                                <div class="col-md-6 input6">
                                                    <input type="text" value="@ViewBag.ContractType" class="form-control" readonly/>
                                                </div>
                                                <div class="texterr"></div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <div class="row">
                                                <label class="col-md-6 label6">رقم العقد</label>
                                                <div class="col-md-6 input6">
                                                    @Html.EditorFor(model => model.CR_Cas_Sup_Branch_Documentation_No, new { htmlAttributes = new { @class = "form-control", @maxlength = "50", autocomplete = "off", @readonly = "readonly" } })
                                                </div>
                                                <div class="texterr"></div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row" hidden>
                                    
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <div class="row">
                                                <label class="col-md-6 label6"> تاريخه </label>
                                                <div class="col-md-6 input6">
                                                    <input class="form-control text-box single-line" data-val="true" data-val-date="The field CR_Cas_Sup_Branch_Documentation_Date must be a date." id="ContractDate" name="ContractDate" type="date" value="">
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-4">
                                        <div class="form-group row">
                                            <label class="col-md-6 label6">تاريخ المستند</label>
                                            @* amir annul deux label     @Html.LabelFor(model => model.CR_Cas_Sup_Branch_Documentation_Date, htmlAttributes: new { @class = "control-label col-md-2" })*@
                                        <div class="col-md-6 input6">
                                            @* amir annul deux input        @Html.EditorFor(model => model.CR_Cas_Sup_Branch_Documentation_Date, new { htmlAttributes = new { @class = "form-control" } })*@
                                            <input class="form-control text-box single-line" data-val="true" data-val-date="The field CR_Cas_Sup_Branch_Documentation_Date must be a date." id="CR_Cas_Sup_Branch_Documentation_Date" name="CR_Cas_Sup_Branch_Documentation_Date" type="date" value="@ViewBag.DocDate">
                                            <div class="texterr">@ViewBag.DocDateError</div>
                                        </div>

                                        </div>
                                    </div>

                                    <div class="col-md-4">
                                        <div class="row form-group">
                                            <label class="col-md-6 label6"> بدايته </label>
                                            @*@Html.LabelFor(model => model.CR_Cas_Sup_Branch_Documentation_Start_Date, htmlAttributes: new { @class = "control-label col-md-2" })*@
                                            <div class="col-md-6 input6">
                                                @*@Html.EditorFor(model => model.CR_Cas_Sup_Branch_Documentation_Start_Date, new { htmlAttributes = new { @class = "form-control" } })*@
                                                <input class="form-control text-box single-line" data-val="true" data-val-date="The field CR_Cas_Sup_Branch_Documentation_Date must be a date." id="CR_Cas_Sup_Branch_Documentation_Start_Date" name="CR_Cas_Sup_Branch_Documentation_Start_Date" type="date" value="@ViewBag.StartDate">
                                                <div class="texterr">@ViewBag.DocStartDateError</div>
                                            </div>
                                            
                                        </div>
                                    </div>

                                    <div class="col-md-4">
                                        <div class="row form-group">
                                            <label class="col-md-6 label6"> نهايته </label>
                                            @*@Html.LabelFor(model => model.CR_Cas_Sup_Branch_Documentation_End_Date, htmlAttributes: new { @class = "control-label col-md-2" })*@
                                            <div class="col-md-6 input6">
                                                <input class="form-control text-box single-line" data-val="true" data-val-date="The field CR_Cas_Sup_Branch_Documentation_Date must be a date." id="CR_Cas_Sup_Branch_Documentation_End_Date" name="CR_Cas_Sup_Branch_Documentation_End_Date" type="date" value="@ViewBag.EndDate">
                                                <div class="texterr">@ViewBag.DocEndDateError</div>
                                            </div>
                                            
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-md-4" hidden>
                                        <div class="form-group row">
                                            <label class="col-md-6 label6">رسم الخدمة</label>
                                            <div class="col-md-6 input6">
                                                <input type="text" class="form-control" value="@ViewBag.FeesPrice" />
                                                <div class="texterr"></div>
                                            </div>

                                        </div>
                                    </div>

                                    <div class="col-md-4">
                                        <div class="form-group row">
                                            <label class="col-md-6 label6">رسم الإشتراك</label>
                                            <div class="col-md-6 input6">
                                                <input type="text" class="form-control" value="@ViewBag.AnnualFees" />
                                                <div class="texterr"></div>
                                            </div>

                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group row">
                                            <label class="col-md-6 label6">نسبة الخصم</label>
                                            <div class="col-md-6 input6">
                                                <input type="text" class="form-control" value="@ViewBag.TaxVal" />
                                            </div>
                                            <div class="texterr"></div>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class=" form-group row">
                                            <label class="col-md-6 label6">نسبة الضريبة</label>
                                            <div class="col-md-6 input6">
                                                <input type="text" class="form-control" value="@ViewBag.Discount" />
                                            </div>
                                            <div class="texterr"></div>
                                        </div>
                                    </div>
                                </div>


                                <div class="col-md-12" hidden="hidden">
                                    <div class="row">
                                        @Html.LabelFor(model => model.CR_Cas_Sup_Branch_Documentation_Credit_Limit, htmlAttributes: new { @class = "control-label col-md-2", style = "float:right" })
                                        <div class="col-md-2 offset-md-8">
                                            @Html.EditorFor(model => model.CR_Cas_Sup_Branch_Documentation_No, new { htmlAttributes = new { @class = "form-control", @maxlength = "50", autocomplete = "off" } })

                                        </div>
                                    </div>
                                </div>
                                <div class="form-group" hidden="hidden">
                                    @Html.LabelFor(model => model.CR_Cas_Sup_Branch_Documentation_Activation, htmlAttributes: new { @class = "control-label col-md-2" })
                                    <div class="col-md-10">
                                        <div class="checkbox">
                                            @Html.EditorFor(model => model.CR_Cas_Sup_Branch_Documentation_Activation)
                                            @Html.ValidationMessageFor(model => model.CR_Cas_Sup_Branch_Documentation_Activation, "", new { @class = "text-danger" })
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group" hidden="hidden">
                                    @Html.LabelFor(model => model.CR_Cas_Sup_Branch_Documentation_About_To_Expire, htmlAttributes: new { @class = "control-label col-md-2" })
                                    <div class="col-md-10">
                                        @Html.EditorFor(model => model.CR_Cas_Sup_Branch_Documentation_About_To_Expire, new { htmlAttributes = new { @class = "form-control" } })
                                        @Html.ValidationMessageFor(model => model.CR_Cas_Sup_Branch_Documentation_About_To_Expire, "", new { @class = "text-danger" })
                                    </div>
                                </div>
                                <div class="form-group" hidden="hidden">
                                    @Html.LabelFor(model => model.CR_Cas_Sup_Branch_Documentation_Status, htmlAttributes: new { @class = "control-label col-md-2" })
                                    <div class="col-md-10">
                                        @Html.EditorFor(model => model.CR_Cas_Sup_Branch_Documentation_Status, new { htmlAttributes = new { @class = "form-control" } })
                                        @Html.ValidationMessageFor(model => model.CR_Cas_Sup_Branch_Documentation_Status, "", new { @class = "text-danger" })
                                    </div>
                                </div>
                                <div class="form-group" hidden="hidden">
                                    @Html.LabelFor(model => model.CR_Cas_Sup_Procedures_Type, htmlAttributes: new { @class = "control-label col-md-2" })
                                    <div class="col-md-10">
                                        @Html.EditorFor(model => model.CR_Cas_Sup_Procedures_Type, new { htmlAttributes = new { @class = "form-control" } })
                                        @Html.ValidationMessageFor(model => model.CR_Cas_Sup_Procedures_Type, "", new { @class = "text-danger" })
                                    </div>
                                </div>

                            </div>
                                <div class="row">

                                    <div class="col-md-6" style="margin-top:10px;margin-bottom:10px;">
                                    </div>
                                    <div class="col-md-6" style="margin-top:10px;margin-bottom:10px;">
                                        <a href="/CasDocumentationContract/Index/" class="btn btn-primary waves-effect" style="float:left">انهاء</a>
                                    </div>
                                </div>

                            
                            }

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script type="text/javascript">
        window.addEventListener("load", function () {

            if ('@TempData["TempModel"]'=="Added") {
                toastr.success("تم الحفظ بنجاح");
            }

            if ('@TempData["TempModel"]'=="Saved") {
                toastr.success("تم الحفظ بنجاح");
            }

            if ('@TempData["TempModel"]'=="Holded") {
                toastr.success("تم التعطيل بنجاح");
            }

            if ('@TempData["TempModel"]'=="Deleted") {
                toastr.success("تم الحذف بنجاح");
            }

            if ('@TempData["TempModel"]'=="Activated") {
                toastr.success("تم التنشيط بنجاح");
            }

        });
</script>