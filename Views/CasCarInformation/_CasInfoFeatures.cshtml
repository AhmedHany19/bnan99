@model IEnumerable<RentCar.Models.MasCasFeaturesModel>

<div class="" style="margin-top:0px;">
    <div class="corp_page">
        <div class="page_content">
            @*<form method="post">*@
            <div class="form_bg">
                <div class="table-responsive" id="tbl" style="width:100%; height:auto; overflow:auto;">
                    <div id="dtBasicExample_print">
                        <table class="table table-striped table_no_resp" id="myTable" style="width:100%; height:250px; overflow-x:hidden;">
                            <thead>
                                <tr>
                                    <th hidden>
                                        @Html.DisplayNameFor(model => model.CR_Cas_Sup_Features_Serial_No)
                                    </th>
                                    <th>
                                        الميزة
                                    </th>
                                    <th hidden>
                                        @Html.DisplayNameFor(model => model.CR_Cas_Sup_Features_Model_Code)
                                    </th>
                                    <th hidden>
                                        @Html.DisplayNameFor(model => model.CR_Cas_Sup_Features_Model_Year)
                                    </th>
                                    <th hidden>
                                        @Html.DisplayNameFor(model => model.CR_Cas_Sup_Features_Lessor_Code)
                                    </th>

                                </tr>
                            </thead>
                            <tbody id="tbl" style="max-height:200px; overflow-x:hidden;">
                                @foreach (var item in Model)
                                {
                                    <tr>
                                        <td hidden>
                                            @Html.DisplayFor(modelItem => item.CR_Cas_Sup_Features_Serial_No)
                                        </td>
                                        <td>
                                            <label class="checkbox_input">
                                                @Html.DisplayFor(modelItem => item.CR_Mas_Sup_Features_Name)
                                                <input type="checkbox" id="chk_@item.CR_Cas_Sup_Features_Code" name="chk_@item.CR_Cas_Sup_Features_Code" checked="@item.check.GetValueOrDefault()" disabled >
                                                <span class="checkmark"></span>
                                            </label>
                                        </td>
                                        <td hidden>
                                            @Html.DisplayFor(modelItem => item.CR_Cas_Sup_Features_Model_Code)
                                        </td>
                                        <td hidden>
                                            @Html.DisplayFor(modelItem => item.CR_Cas_Sup_Features_Model_Year)
                                        </td>
                                        <td hidden>
                                            @Html.DisplayFor(modelItem => item.CR_Cas_Sup_Features_Lessor_Code)
                                        </td>

                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
            @*</form>*@
        </div>
    </div>
</div>
