@model IEnumerable<RentCar.Models.CR_Cas_Sup_Car_Doc_Mainten>

<div class="responsive-table-plugin">
    <div class="table-rep-plugin">
        <div class="table-responsive" data-pattern="priority-columns" style="border:none;">
            <div id="dtBasicExample_print">
                <table id="myTable" class="table table-striped mb-0" style="cursor:pointer">
                    <thead>
                        <tr>
                            <th width="10%">
                                التسلسلي
                            </th>
                            <th width="34%">
                                السيارة
                            </th>
                            <th width="10%">
                                نوع الوثيقة
                            </th>

                            <th width="20%">
                                رقم الوثيقة
                            </th>


                            <th width="8%">
                                بدايته
                            </th>
                            <th width="8%">
                                نهايته
                            </th>

                            <th width="10%">
                                الحالة
                            </th>

                        </tr>
                    </thead>
                    <tbody id="tbl">

                        @foreach (var item in Model)
                        {
                            <tr>

                                <td style="text-align:center;vertical-align:middle" width="10%">
                                    <a href="~/CarDocMainten/edit/?id1=@Html.DisplayFor(modelItem => item.CR_Cas_Sup_Car_Doc_Mainten_Serail_No)
                                                                               &id2=@Html.DisplayFor(modelItem => item.CR_Cas_Sup_Car_Doc_Mainten_Code)">
                                        @Html.DisplayFor(modelItem => item.CR_Cas_Sup_Car_Doc_Mainten_Serail_No)
                                    </a>
                                </td>

                                <td style="vertical-align:middle" width="34%">
                                    <a href="~/CarDocMainten/edit/?id1=@Html.DisplayFor(modelItem => item.CR_Cas_Sup_Car_Doc_Mainten_Serail_No)
                                                                               &id2=@Html.DisplayFor(modelItem => item.CR_Cas_Sup_Car_Doc_Mainten_Code)">
                                        @Html.DisplayFor(modelItem => item.CR_Cas_Sup_Car_Information.CR_Cas_Sup_Car_Collect_Ar_Name)
                                    </a>
                                </td>

                                <td style="vertical-align:middle"width="10%">
                                    <a href="~/CarDocMainten/edit/?id1=@Html.DisplayFor(modelItem => item.CR_Cas_Sup_Car_Doc_Mainten_Serail_No)
                                                                               &id2=@Html.DisplayFor(modelItem => item.CR_Cas_Sup_Car_Doc_Mainten_Code)">
                                        @Html.DisplayFor(modelItem => item.CR_Mas_Sup_Procedures.CR_Mas_Sup_Procedures_Ar_Name)
                                    </a>
                                </td>


                                <td style="text-align:center;vertical-align:middle"width="20%">
                                    <a href="~/CarDocMainten/edit/?id1=@Html.DisplayFor(modelItem => item.CR_Cas_Sup_Car_Doc_Mainten_Serail_No)
                                                                               &id2=@Html.DisplayFor(modelItem => item.CR_Cas_Sup_Car_Doc_Mainten_Code)">
                                        @Html.DisplayFor(modelItem => item.CR_Cas_Sup_Car_Doc_Mainten_No)
                                    </a>
                                </td>

                                @*<td style="text-align:center;vertical-align:middle">
                                        @if (item.CR_Cas_Sup_Car_Doc_Mainten_Date != null)
                                        {
                                            <a href="~/CarDocMainten/edit/?id1=@Html.DisplayFor(modelItem => item.CR_Cas_Sup_Car_Doc_Mainten_Serail_No)
                                                   &id2=@Html.DisplayFor(modelItem => item.CR_Cas_Sup_Car_Doc_Mainten_Code)">
                                                @item.CR_Cas_Sup_Car_Doc_Mainten_Date.Value.ToString("yyyy/MM/dd")
                                            </a>
                                        }
                                    </td>*@

                                <td style="text-align:center;vertical-align:middle" width="8%">
                                    @if (item.CR_Cas_Sup_Car_Doc_Mainten_Start_Date != null)
                                    {
                                        <a href="~/CarDocMainten/edit/?id1=@Html.DisplayFor(modelItem => item.CR_Cas_Sup_Car_Doc_Mainten_Serail_No)
                                                                               &id2=@Html.DisplayFor(modelItem => item.CR_Cas_Sup_Car_Doc_Mainten_Code)">
                                            @item.CR_Cas_Sup_Car_Doc_Mainten_Start_Date.Value.ToString("yyyy/MM/dd")
                                        </a>
                                    }

                                </td>

                                @*<td style="text-align:center">
                                        @Html.CheckBox("CR_Cas_Sup_Car_Doc_Mainten_Activation", item.CR_Cas_Sup_Car_Doc_Mainten_Activation.GetValueOrDefault(), new { @class = "text", disabled = true })
                                    </td>*@
                                <td style="text-align:center;vertical-align:middle" width="8%">
                                    @if (item.CR_Cas_Sup_Car_Doc_Mainten_End_Date != null)
                                    {
                                        <a href="~/CarDocMainten/edit/?id1=@Html.DisplayFor(modelItem => item.CR_Cas_Sup_Car_Doc_Mainten_Serail_No)
                                                                               &id2=@Html.DisplayFor(modelItem => item.CR_Cas_Sup_Car_Doc_Mainten_Code)">
                                            @item.CR_Cas_Sup_Car_Doc_Mainten_End_Date.Value.ToString("yyyy/MM/dd")
                                        </a>
                                    }
                                </td>

                                <td style="text-align:center;vertical-align:middle">
                                    <a href="~/CarDocMainten/edit/?id1=@Html.DisplayFor(modelItem => item.CR_Cas_Sup_Car_Doc_Mainten_Serail_No)
                                                                               &id2=@Html.DisplayFor(modelItem => item.CR_Cas_Sup_Car_Doc_Mainten_Code)">
                                        @if (item.CR_Cas_Sup_Car_Doc_Mainten_Status == "N")
                                        {

                                            <label style="color:black;">لم يسجل</label>


                                        }
                                        else if (item.CR_Cas_Sup_Car_Doc_Mainten_Status == "D")
                                        {

                                            <label style="color:red">محذوف</label>


                                        }
                                        else if (item.CR_Cas_Sup_Car_Doc_Mainten_Status == "A")
                                        {

                                            <label style="color:green;">نشط</label>


                                        }
                                        else if (item.CR_Cas_Sup_Car_Doc_Mainten_Status == "E")
                                        {

                                            <label style="color:dimgrey;">منتهي</label>


                                        }
                                        else if (item.CR_Cas_Sup_Car_Doc_Mainten_Status == "X")
                                        {

                                            <label style="color:orange">على وشك الإنتهاء</label>


                                        }
                                    </a>
                                </td>
                                <td hidden>
                                    @Html.DisplayFor(modelItem => item.CR_Cas_Sup_Car_Doc_Mainten_Type)
                                </td>
                                <td hidden>
                                    @Html.DisplayFor(modelItem => item.CR_Mas_Sup_Procedures.CR_Mas_Sup_Procedures_Type)
                                </td>
                                <td hidden>
                                    @Html.DisplayFor(modelItem => item.CR_Cas_Sup_Car_Doc_Mainten_Serail_No)
                                </td>
                            </tr>

                        }

                    </tbody>
                </table>
            </div>
            <iframe name="print_frame" width="0" height="0" frameborder="0" src="about:blank"></iframe>
        </div>
    </div>
</div>
