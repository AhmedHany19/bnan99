@model IEnumerable<RentCar.Models.BranchCarsModel>


<table id="myTable" class="table table-striped mb-0" style="cursor:pointer">
    <thead>
    <tr>
        <th onclick="sortTable(0)" width="11%">
            الفرع
        </th>
        <th onclick="sortTable(2)" width="23%">
            المدينة
        </th>

        <th onclick="sortTable(3)" width="21%">
            المدير
        </th>
        <th onclick="sortTable(4)" width="14%">
