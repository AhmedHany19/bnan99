                            <a href="~/CasBranch/edit/?id1=@Html.DisplayFor(modelItem => item.CR_Cas_Sup_Branch_Code)&id2=@Html.DisplayFor(modelItem => item.CR_Cas_Sup_Lessor_Code)&id3=@Html.DisplayFor(modelItem => item.CR_Cas_Sup_Branch_Government_No)" style="color:green">
                                نشط
                            </a>
                        }
                        else if (item.CR_Cas_Sup_Branch_Status == "H")
