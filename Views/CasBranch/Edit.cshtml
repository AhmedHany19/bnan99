            document.getElementById("save").click();
        } else if (st == 'D') {
            toastr.info("لايمكن حفظ بيانات فرع محذوف");
        } else if (st == 'H') {
            toastr.info("لايمكن حفظ بيانات فرع موقوف");
        }
    }

