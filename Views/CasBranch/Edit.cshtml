    var modalConfirm = function (callback) {
        $("#btn-confirm").on("click", function () {
            var st = '@Model.CR_Cas_Sup_Branch_Status';
            if (st == 'A') {
                var CarsNo = '@TempData["CarsNo"]';
                var SalesPointNo = '@TempData["SalesPointNo"]';

                var x = false;


                if (@Model.CR_Cas_Sup_Branch_Code != "100")
                {
                    x = true;
                }
                else
                {
                    toastr.info("لايمكن حذف الفرع الرئيسي");
                    return;
                }

                if (CarsNo === "True") {
                    toastr.info("لايمكن حذف فرع به سيارات");
                    return;
                } else {
                    x = true;
                }


                if (SalesPointNo==="True") {
                    console.log(@Model.CR_Cas_Sup_SalesPoint.Where(l=>l.CR_Cas_Sup_SalesPoint_Status == "A").Count())
                    toastr.info("لايمكن حذف فرع به نقاط بيع");
                    return;

                } else {
                    x = true;
                }

                if (x == true) {
                    $("#mi-modal").modal('show');
                }
            } else if (st == 'D') {
                $("#mi-modal").modal('show');
            } else if (st == 'H') {
                toastr.info("لايمكن حذف فرع موقوف");
            }


           // $("#mi-modal").modal('show');

        });
        $("#modal-btn-si").on("click", function () {
            callback(true);
            $("#mi-modal").modal('hide');
        });
        $("#modal-btn-no").on("click", function () {
            callback(false);
            $("#mi-modal").modal('hide');
        });
    };
    modalConfirm(function (confirm) {
        if (confirm) {
            //Acciones si el usuario confirma
            $("#form1").submit();
        } else {
            //Acciones si el usuario no confirma
            $("#result").html("canceled");
        }
    });
</script>
