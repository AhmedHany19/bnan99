                                                    <input type="time" class="form-control" id="CR_Cas_Sup_Branch_Duration_Three_End_Time" name="CR_Cas_Sup_Branch_Duration_Three_End_Time" onkeyup="movetoNext('CR_MAS_SUP_City_Code')" value="@ViewBag.ET3">
