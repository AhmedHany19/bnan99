                                        <div class="col-md-10">
                                            @Html.EditorFor(model => model.CR_Cas_Sup_Branch_Code, new { htmlAttributes = new { @class = "form-control" } })
                                            @Html.ValidationMessageFor(model => model.CR_Cas_Sup_Branch_Code, "", new { @class = "text-danger" })
                                        </div>
                                    </div>

                                    <div class="form-group" hidden="hidden">
