                                                <div class="col-md-9 col-md-6 inputresp6 padding_right_none">
                                                    <input type="text" value="@ViewBag.B_En_Name" class="form-control" id="CR_Cas_Sup_Branch_En_Name" autocomplete="off" name="CR_Cas_Sup_Branch_En_Name" maxlength="100" required="" oninput="this.value=this.value.replace(/[^a-zA-Z\s]/g,'')" onkeyup="movetoNext('CR_Cas_Sup_Branch_En_Short_Name')" onpaste="return false;">
                                                    <div><span id="textErr">@ViewBag.LRExistEnLng</span></div>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="col-lg-4 col-md-12">
                                            <div class="form-group row">
                                                <label class="col-md-6 col-form-label label6 labelresp6 padding_left_none" for="CR_Cas_Sup_Branch_En_Short_Name">إسم الفرع مختصر<label id="asterisk">*</label></label>
                                                <div class="col-md-6 input6 padding_right_none">
                                                    <input type="text" value="@ViewBag.B_En_Sh_Name" class="form-control" id="CR_Cas_Sup_Branch_En_Short_Name" autocomplete="off" name="CR_Cas_Sup_Branch_En_Short_Name" maxlength="15" required="" oninput="this.value=this.value.replace(/[^a-zA-Z\s]/g,'')" onkeyup="movetoNext('CR_Cas_Sup_Branch_Government_No')" onpaste="return false;">
                                                    <div><span id="textErr">@ViewBag.LRExistEnSh</span></div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

