                                                @*<div class="col-md-6 label6" style=" float:right">

                                                    <label id="CR_Cas_Sup_Branch_Signature_Director" class="lbl " style=" float:right">@Html.LabelFor(model => model.CR_Cas_Sup_Branch_Signature_Director)<label id="asterisk">*</label></label>
                                                </div>*@
                                                <div class="col-md-6 input6" style=" float:right">
                                                    <div class="input-group padding_right_none">
                                                        <span class="input-group-btn" style="width:100%">
                                                            <span class="btn btn-primary" onclick="$(this).parent().find('input[type=file]').click();" style="width:100%;height:35px;width:100%;line-height:20px;">توقيع المدير</span>
                                                            <input name="imgDirector" type="file" class="form-control " style="display:none;float:right;width:100%; object-fit: fill;" accept="image/*" id="CR_Cas_Sup_Branch_Signature_Director" onchange="loadFilesignature(event)" onkeyup="movetoNext('CR_Cas_Sup_Branch_Start_Day')">
                                                        </span>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="texterr" style="float:right"></div>
                                        </div>
                                        <div class="col-lg-4 col-md-6 padding_right_none" style="float:right;">
                                            <img id="signature" class="zoom" width="120" height="60" style="margin-top: -14px; float: right; margin-right: 0; width: 120px; height: 60px; position: relative; object-fit: cover; border: solid 1px transparent" />
                                        </div>
                                    </div>

                                    <div class="row" hidden>
                                        <div class="col-lg-4 col-md-12">
                                            <div class="form-group row">
                                                <label class="col-md-6 col-form-label label6 labelresp6 padding_left_none" for="CR_Cas_Sup_Branch_Signature_Ar_Director_Name">بداية الأسبوع</label>
                                                <div class="col-md-6 input6 padding_right_none">
                                                    @Html.DropDownList("listStart", ViewData["listStart"] as SelectList, new { @class = "form-control", style = "float:right", Required = "Required" })
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-lg-4 col-md-12">
                                            <div class="form-group row">
                                                <label class="col-md-6 col-form-label label6 padding_left_none" for="CR_Cas_Sup_Branch_Start_Day">نهاية الأسبوع</label>
                                                <div class="col-md-6 input6 padding_right_none">
                                                    @Html.DropDownList("listEnd", ViewData["listEnd"] as SelectList, new { @class = "form-control", style = "float:right", Required = "Required" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>


                                    <div class="row" hidden>
                                        <div class="col-lg-4 col-md-12">
                                            <div class="form-group row">
                                                <label class="col-md-6 col-form-label label6 padding_left_none" for="CR_Cas_Sup_Branch_Duration_One_Start_Time">بداية الدوام الأول<label id="asterisk">*</label></label>
                                                <div class="col-md-6 input6 padding_right_none">
                                                    <input type="time" class="form-control" id="CR_Cas_Sup_Branch_Duration_One_Start_Time" name="CR_Cas_Sup_Branch_Duration_One_Start_Time"  onkeyup="movetoNext('CR_Cas_Sup_Branch_Duration_One_End_Time')" value="@ViewBag.ST1">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-lg-4 col-md-12">
                                            <div class="form-group row">
                                                <label class="col-md-6 col-form-label label6 padding_left_none" for="CR_Cas_Sup_Branch_Duration_One_End_Time">نهاية الدوام<label id="asterisk">*</label></label>
                                                <div class="col-md-6 input6 padding_right_none">
                                                    <input type="time" class="form-control" id="CR_Cas_Sup_Branch_Duration_One_End_Time" name="CR_Cas_Sup_Branch_Duration_One_End_Time"  onkeyup="movetoNext('CR_Cas_Sup_Branch_Duration_Tow_Start_Time')" value="@ViewBag.ET1">
                                                </div>
                                            </div>
                                        </div>
