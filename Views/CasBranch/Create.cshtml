                            @*using (Html.BeginForm("Create", "CasBranch", FormMethod.Post, new { enctype = "multipart/form-data", id = "form_validation", novalidate = "" }))*@

                            <form id="form_validation" action="/CasBranch/Create" enctype="multipart/form-data" method="post" data-parsley-validate novalidate>

                                @Html.AntiForgeryToken()
                                <div class="form-horizontal">
                                    @Html.ValidationSummary(true, "", new { @class = "text-danger" })


