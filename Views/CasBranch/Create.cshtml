@model RentCar.Models.CR_Cas_Sup_Branch

@{
    Layout = "~/Views/Shared/_CASLayout.cshtml";
}


<style>
    #signature {
        display: none
    }

    #textErr {
        color: red;
        font-size: 12px;
        font-weight: bold;
    }
</style>
<div class="content">

    <!-- Start Content-->
    <div class="container-fluid">

        <div class="row">
            <div class="col-12">
                <div class="card-box table-responsive" style="height:79vh">
                    <div class="title_corp"><h3> الشركة / الفروع /إضافة </h3></div>
                    <div class="subtitle_corp">تمكن هذه الصفحة المستخدم من إضافة فرع جديد</div>
                    <div class="hr_60"></div>
                    <div class="hr_80"></div>
                    <div class="corp_page " style=" padding-top: 20px;">
