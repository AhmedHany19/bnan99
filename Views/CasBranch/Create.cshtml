                                                <label class="col-md-6 col-form-label label6 labelresp6 padding_left_none" for="CR_Cas_Sup_Branch_Mobile">رقم الجوال</label>
                                                <div class="col-md-6 input6 padding_right_none">
                                                    <input type="text" value="@ViewBag.Mobile" class="form-control" id="CR_Cas_Sup_Branch_Mobile" name="CR_Cas_Sup_Branch_Mobile" autocomplete="off" oninput="this.value=this.value.replace(/[^0-9]/g,'')" maxlength="15" onkeyup="movetoNext('CR_Cas_Sup_Branch_Signature_Ar_Director_Name')" onpaste="return false;">
                                                </div>
                                            </div>
