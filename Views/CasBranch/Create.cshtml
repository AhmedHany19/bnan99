                                                <div class="col-md-6 input6 padding_right_none">
                                                    <input type="time" class="form-control" id="CR_Cas_Sup_Branch_Duration_Tow_Start_Time" name="CR_Cas_Sup_Branch_Duration_Tow_Start_Time" onkeyup="movetoNext('CR_Cas_Sup_Branch_Duration_Tow_End_Time')" value="@ViewBag.ST2">
