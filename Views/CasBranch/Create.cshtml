                                                <div class="col-md-6 input6 padding_right_none">
                                                    <input type="text" class="form-control" id="CR_Mas_Address_Ar_District" autocomplete="off" name="CR_Mas_Address_Ar_District" maxlength="50" required="" onkeypress="return CheckArabicCharactersOnly(event);" onkeyup="movetoNext('CR_Mas_Address_En_District')" onpaste="return false;" value="@ViewBag.DistrictAr">
