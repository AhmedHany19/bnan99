                                                <label class="lbl col-md-6 label6 padding_left_none" style="float:right;">إسم المدينة<label id="asterisk">*</label></label>
                                                <div class="col-md-6 input6 select_error padding_right_none">
                                                    @Html.DropDownList("CR_MAS_SUP_City_Code", null, "--- الرجاء إختر المدينة ---", new { @class = "form-control", style = "float:right;width:100%", onkeyup = "movetoNext('CR_MAS_SUP_Region_Ar_Name')", required="required" })

                                                </div>
                                            </div>
                                            <div class="texterr" style="float:right">@ViewBag.CityCode</div>
                                        </div>


